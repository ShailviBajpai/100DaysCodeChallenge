QUESTION: https://www.hackerrank.com/challenges/java-strings-introduction/problem?isFullScreen=true

SOLUTION: import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        String B=sc.next();
        int sum= A.length()+ B.length();
        System.out.println(sum);
        if(A.compareTo(B)>0)
        {
            System.out.println("Yes");
        }
        else{
            System.out.println("No");
        }
        char a= Character.toUpperCase(A.charAt(0));
        char b= Character.toUpperCase(B.charAt(0));
         System.out.println(a+A.substring(1)+" "+b+B.substring(1));
    }
}
