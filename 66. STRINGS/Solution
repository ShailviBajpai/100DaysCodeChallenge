QUESTION: https://www.hackerrank.com/challenges/java-substring/problem?isFullScreen=true

SOLUTION: import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        String S = in.next();
        int start = in.nextInt();
        int end = in.nextInt();
        String s1= "";
        for(int i=start; i<end; i++)
    {
        s1= s1+S.charAt(i);
    }
    System.out.print(s1);
    }
}
